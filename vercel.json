{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "redirects": [
    { "source": "/icoone-laser", "destination": "/icoone-laser-muenchen", "permanent": true },
    { "source": "/redtouchpro", "destination": "/redtouch-laser-muenchen", "permanent": true },
    { "source": "/manikuere-pedikuere", "destination": "/manikuere-pedikuere-muenchen", "permanent": true },
    { "source": "/kopie-deka-day-anna", "destination": "/deka-anna", "permanent": true }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/.*\\.(jpg|jpeg|png|gif|ico|svg|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=86400"
        }
      ]
    }
  ]
}
